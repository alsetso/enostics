# Production Environment Variables for Enostics
# Copy this to .env.production.local and fill in real values

# Supabase Configuration (Production)
NEXT_PUBLIC_SUPABASE_URL=https://your-production-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_production_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_production_service_role_key

# OpenAI API Configuration
OPENAI_API_KEY=your_production_openai_api_key

# Application Configuration
NEXT_PUBLIC_APP_URL=https://enostics.com
NEXT_PUBLIC_API_URL=https://api.enostics.com
NODE_ENV=production

# Security & Rate Limiting
RATE_LIMIT_WINDOW_MS=3600000
RATE_LIMIT_MAX_REQUESTS=1000
API_KEY_SECRET=your_secure_api_key_secret

# Monitoring & Analytics
NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id
SENTRY_DSN=your_sentry_dsn

# Feature Flags
NEXT_PUBLIC_ENABLE_AI_FEATURES=true
NEXT_PUBLIC_ENABLE_PREMIUM_FEATURES=true
NEXT_PUBLIC_MAINTENANCE_MODE=false 